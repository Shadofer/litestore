{
  "name": "litestore-srv",
  "version": "1.0.0",
  "private": true,
  "scripts": {
    "build": "swc src -d output && copyfiles generated/* generated/local/** output",
    "start": "run-s build start:cached",
    "start:cached": "node -r @swc-node/register -r tsconfig-paths/register output/src/api.js",
    "dev": "tsnd --ignore-watch generated/ --transpile-only --respawn --exit-child --clear -r tsconfig-paths/register src/api.ts",
    "test": "cross-env NODE_ENV=test node --experimental-vm-modules ./node_modules/.bin/jest --setTimeout=2500 --runInBand --detectOpenHandles --forceExit",
    "check": "tsc"
  },
  "dependencies": {
    "@ezier/validate": "^0.2.3",
    "@prisma/client": "^5.12.1",
    "@swc-node/register": "^1.9.0",
    "@swc/cli": "^0.3.10",
    "bcrypt": "^5.1.1",
    "copyfiles": "^2.4.1",
    "dotenv": "^16.4.5",
    "express": "^4.18.3",
    "imagekit": "^5.0.1",
    "jsonwebtoken": "^9.0.2",
    "npm-run-all": "^4.1.5",
    "prisma": "^5.12.1",
    "tsconfig-paths": "^4.2.0",
    "tslib": "^2.6.2",
    "uuid": "^9.0.1"
  },
  "devDependencies": {
    "@babel/preset-typescript": "^7.23.3",
    "@jest/globals": "^29.7.0",
    "@types/bcrypt": "^5.0.2",
    "@types/express": "^4.17.21",
    "@types/jest": "^29.5.12",
    "@types/jsonwebtoken": "^9.0.6",
    "@types/supertest": "^6.0.2",
    "@types/uuid": "^9.0.8",
    "cross-env": "^7.0.3",
    "jest": "^29.7.0",
    "superagent-defaults": "^0.1.14",
    "supertest": "^6.3.4",
    "ts-jest": "^29.1.2",
    "ts-node-dev": "^2.0.0"
  }
}
